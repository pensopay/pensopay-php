{
    "name": "pensopay/pensopay-php",
    "version": "1.0.0",
    "description": "Acquire your payments using PHP with the pensopay API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "pensopay"
    ],
    "homepage": "https://pensopay.com",
    "license": "MIT",
    "authors": [
        {
            "name": "pensopay A/S",
            "homepage": "https://pensopay.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Pensopay\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Pensopay\\Client\\Test\\" : "test/" }
    }
}
